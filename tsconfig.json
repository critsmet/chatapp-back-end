{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "erasableSyntaxOnly": true,
    "noImplicitAny": true,
  },
  "include": ["./*.ts"],
  "exclude": ["node_modules"]
}